{
  "Routes": [
    //Category
    {
      "UpstreamPathTemplate": "/api/Category/GetAllCategory",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "DownstreamPathTemplate": "/Category/GetAllCategory"
    },
    {
      "UpstreamPathTemplate": "/api/Category/CreateCategory",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "DownstreamPathTemplate": "/Category/CreateCategory"
    },
    {
      "UpstreamPathTemplate": "/api/Category/EditCategory/{id}",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "DownstreamPathTemplate": "/Category/EditCategory/{id}"
    },
    {
      "UpstreamPathTemplate": "/api/Category/DeleteCategory/{id}",
      "UpstreamHttpMethod": [ "Delete" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "DownstreamPathTemplate": "/Category/DeleteCategory/{id}"
    },
    {
      "UpstreamPathTemplate": "/api/Category/GetOneCategory/{id}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "DownstreamPathTemplate": "/Category/GetOneCategory/{id}"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:800"
  }
}